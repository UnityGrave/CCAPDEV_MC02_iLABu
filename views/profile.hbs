<h2>Reservations:</h2>
<ul id="reservations-container">
    {{#each userReservations}}
        <li>
            <h3>{{ building }} {{ roomnumber }}</h3>
            <p>{{ date.toLocaleDateString() }}</p>
            <p>Time: {{ startTime.toLocaleTimeString() }} - {{ endTime.toLocaleTimeString() }}</p>
        </li>
    {{/each}}
</ul>